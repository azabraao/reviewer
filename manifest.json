{
    "manifest_version": 2,
    "name": "Reviewer",
    "description": "Review for Trello",
    "version": "0.0.1",
    "icons": {
        "128": "128.png"
    },
    "browser_action":{
        "default_popup":"popup.html"
    },
    "permissions":[
        "activeTab"
    ],
    "content_scripts":[
        {
            "matches": ["https://*.trello.com/*"],
            "js": ["index.js"]
        }
    ]
}